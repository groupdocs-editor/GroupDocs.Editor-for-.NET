{
  "LocalStorageOptions": {
    "RootFolder": "pathToStorage",
    "BaseUrl": "https://yourBaseUrl"
  },
  "AWS": {
    "Profile": "s3-dotnet-demo",
    "Bucket": "groupdocs-editor",
    "RootFolderName": "groupdocseditor",
    "LinkExpiresDays": 360,
    "Region": "us-west-2",
    "AccessKey": "<AWS_AccessKey>",
    "SecretKey": "<AWS_SecretKey>"
  },
  "AzureBlobOptions": {
    "AccountName": "s3-dotnet-demo",
    "AccountKey": "us-west-2",
    "ContainerName": "groupDocs-editor-files",
    "LinkExpiresDays": 360
  },
  "LicenseOptions": {
    "Type": 1,
    "Source": "https://docs.groupdocs.com/editor/net/licensing-and-subscription/"
  },
  "FeatureManagement": {
    "WordProcessing": true,
    "LocalFile": true,
    "Pdf": true,
    "Presentation": true,
    "Spreadsheet": true,
    "Email": true
  }
}
